<%= form_for @article do |f| %>
<% if @article.errors.any? %>
<div id='error explanation' %>
	<h2><%=pluralize(@article.errors.count,'error') %>Prohibited article from being saved: </h2>
	<ul>
		<% @article.errors.full_messages.each do |message| %>
		<li><%= message %></li>
         <% end %>
	</ul>
    </div>
   <% end %>
<p>
	<%= f.label :name %>
	<%= f.text_field :name %>
</p>
<p>
	<%= f.label :age %>
	<%= f.text_field :age %>
</p>
<p>
	<%= f.label :email %>
	<%= f.text_field :email %>
</p>
<p>
	<%= f.label :resume %>
	<%= f.text_field :resume %>
</p>
<p>
	<%= f.submit :clickme %>
</p>
<% end %>